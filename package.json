    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "jest": "^23.6.0"
  },
  "dependencies": {
