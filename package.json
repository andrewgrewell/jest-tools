    "build": "rm -rf dist && babel src -d dist --ignore __tests__ && cp -R ./src/reporter ./dist/reporter",
    "prepublish": "npm run build"
  },
  "bin": {
    "jest-tools": "./dist/cli.js"
  },
  "author": "Andrew Grewell <andrewgrewell@gmail.com>",
